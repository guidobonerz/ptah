{
    "nameSpace": "de.drazil.test",
    "childProjectNames": [
        "path-to-project-json-files"
    ],
    "targets": [
        {
            "name": "database",
            "type": "mssql",
            "suffix": "sql",
            "allowBundledFile": true,
            "dataTypes": [
                {
                    "genericType": "string",
                    "realType": "nvarchar2",
                    "hasSize": true,
                    "maxSize": "MAX"
                },
                {
                    "genericType": "integer",
                    "realType": "bigint",
                    "hasSize": false
                },
                {
                    "genericType": "date",
                    "realType": "date",
                    "hasSize": false
                },
                {
                    "genericType": "boolean",
                    "realType": "bit",
                    "hasSize": false
                }
            ]
        }
    ],
    "entities": [
        {
            "name": "test",
            "schema": "dbo",
            "type": "Table",
            "enableCreate": true,
            "enableRead": true,
            "enableUpdate": true,
            "enableDelete": true,
            "generateHistoryEntity": true,
            "addDefaultAttributes": true,
            "attributes": [
                {
                    "primaryKey": true,
                    "name": "id",
                    "dataType": "integer",
                    "allowNull": true,
                    "autoId": true
                },
                {
                    "name": "column1",
                    "dataType": "integer",
                    "primaryKey": true,
                    "defaultValue": "0",
                    "allowNull": true
                },
                {
                    "name": "column2",
                    "dataType": "string",
                    "primaryKey": false,
                    "allowNull": true
                },
                {
                    "name": "column3",
                    "dataType": "date",
                    "primaryKey": false,
                    "defaultValue": "date()",
                    "allowNull": false
                },
                {
                    "name": "column4",
                    "dataType": "boolean",
                    "primaryKey": false,
                    "defaultValue": "0",
                    "allowNull": false
                }
            ]
        }
    ]
}