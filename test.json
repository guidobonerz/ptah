{
    "baseNameSpace": "de.drazil.test.generated",
    "childProjectNames": [
        "path-to-project-json-files"
    ],
    "templateConfigurations": [
        {
            "templateBasePath": "database/mssql/",
            "suffix": "sql",
            "allowBundledFile": true,
            "dataTypes": {
                "string": {
                    "dataType": "nvarchar",
                    "hasSize": true,
                    "maxSize": "MAX"
                },
                "integer": {
                    "dataType": "bigint",
                    "hasSize": false
                },
                "date": {
                    "dataType": "date",
                    "hasSize": false
                },
                "boolean": {
                    "dataType": "bit",
                    "hasSize": false
                }
            },
            "templates": [
                {
                    "name": "table",
                    "nameSpace": "",
                    "namePattern": "%s",
                    "enabled": false
                },
                {
                    "name": "view",
                    "nameSpace": "",
                    "namePattern": "%sView"
                },
                {
                    "name": "procedure",
                    "namePattern": "%s",
                    "nameSpace": ""
                }
            ]
        },
        {
            "templateBasePath": "src/",
            "suffix": "java",
            "allowBundledFile": false,
            "dataTypes": {
                "string": {
                    "dataType": "java.lang.String"
                },
                "integer": {
                    "dataType": "java.lang.Long"
                },
                "date": {
                    "dataType": "java.util.Date"
                },
                "boolean": {
                    "dataType": "java.lang.Boolean"
                }
            },
            "templates": [
                {
                    "name": "dto",
                    "nameSpace": "dto",
                    "namePattern": "%sDTO",
                    "enabled": true
                },
                {
                    "name": "service",
                    "nameSpace": "service",
                    "namePattern": "I%sService",
                    "enabled": true
                },
                {
                    "name": "impl",
                    "nameSpace": "service.impl",
                    "namePattern": "%sServiceImpl",
                    "enabled": true
                },
                {
                    "name": "dao",
                    "nameSpace": "service.dao",
                    "namePattern": "%sDAO",
                    "enabled": true
                }
            ]
        }
    ],
    "entities": [
        {
            "name": "test",
            "schema": "dbo",
            "enableCreate": true,
            "enableRead": true,
            "enableUpdate": true,
            "enableDelete": true,
            "generateHistoryObject": true,
            "addDefaultAttributes": true,
            "attributes": [
                {
                    "name": "id",
                    "primaryKey": true,
                    "dataType": "integer",
                    "allowNull": false,
                    "autoId": true
                },
                {
                    "name": "column1",
                    "primaryKey": false,
                    "dataType": "integer",
                    "defaultValue": "0",
                    "allowNull": true
                },
                {
                    "name": "column2",
                    "dataType": "string",
                    "allowNull": true
                },
                {
                    "name": "column3",
                    "dataType": "date",
                    "defaultValue": "date()",
                    "allowNull": false
                },
                {
                    "name": "column4",
                    "dataType": "boolean",
                    "defaultValue": "0",
                    "allowNull": false
                }
            ]
        }
    ]
}